<%= link_to "収入科目一覧に戻る", incomes_path %>
<h2>収入科目登録</h2>
  <%= form_with model: @income, method: :post, local: true do |form| %>
  <table>
  <tr>
    <th><%= form.label :name, "名称" %></th>
    <td><%= form.text_field :name %></td>
  </tr>
  <tr>
    <th><%= form.label :description, "詳細" %></th>
    <td><%= form.text_field :description %></td>
  </tr>
</table>
<div><%= form.submit "登録する" %></div>
<% end %>
